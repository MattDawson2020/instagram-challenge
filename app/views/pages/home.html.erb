<h2> InstaJam </h2>


<%= form_for(:session , url: login_path, remote: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %> 
  <%= f.submit "Log in" %>
<% end %>

<%= link_to "Sign up", new_user_path %>